{
  "homepage": "https://jhonjara.github.io/Flujos_y_Niveles",
  "scripts": {
    "start": "node --no-deprecation node_modules/http-server/bin/http-server . -p 8000 -c-1",
    "debug-server": "node --trace-deprecation node_modules/http-server/bin/http-server . -p 8000",
    "watch": "browser-sync start --server . --files \"index.html style.css script.js diagnostic.js img/**/*\""
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "browser-sync": "^2.27.10"
  }
}
